{
  "name": "auto-config-env",
  "displayName": "auto_config_env",
  "description": "An extension to auto config environment of python project with LLMs.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.runPythonAndHandleErrors",
        "title": "Run Python and Handle Errors"
      },
      {
          "command": "extension.completeImports",
          "title": "Complete Imports"
      },
      {
        "command": "extension.generateRequirements",
        "title": "Genearte Requirements"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "SidebarAutoRun",
          "title": "Auto Config",
          "icon": "resources/heart.png"
        }
      ]
    },
    "views": {
      "SidebarAutoRun": [
        {
          "type": "webview",
          "id": "view.autorun",
          "name": "Auto Run Script"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.91.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "openai": "^4.52.7"
  }
}
